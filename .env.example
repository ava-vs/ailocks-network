# Neon PostgreSQL Database URL
DATABASE_URL=postgresql://neondb

# AI APIs
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=ant-your-anthropic-key
OPENROUTER_API_KEY=sk-or-your-openrouter-key

# Security
JWT_SECRET=your-jwt-secret-key

# Netlify - REQUIRED: Replace with your actual values
# Get NETLIFY_SITE_ID from your Netlify site's general settings
# Generate NETLIFY_AUTH_TOKEN from your Netlify user settings (Personal Access Token)
NETLIFY_SITE_ID=
NETLIFY_AUTH_TOKEN=r-auth-token

# Optional: Tavus for video chat
TAVUS_API_KEY=tavus-your-key

ELEVENLABS_API_KEY="your-elevenlabs-api-key"
AGENT_ID="your-elevenlabs-agent-id"

STRIPE_SECRET_KEY=sk_test_...
STRIPE_PRICE_PRO_MONTHLY=price_...
STRIPE_PRICE_ENTERPRISE_MONTHLY=price_...

# IMPORTANT: Create a .env file with your actual values
# Copy this file to .env and replace with real credentials