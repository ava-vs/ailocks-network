{
  "name": "ailocks-ai2ai-network",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "dev:netlify": "netlify dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "db:setup": "node scripts/setup-env.cjs",
    "db:generate": "npm run db:setup && drizzle-kit generate",
    "db:push": "npm run db:setup && drizzle-kit push",
    "db:studio": "npm run db:setup && drizzle-kit studio",
    "db:reset": "npm run db:setup && drizzle-kit push --force",
    "db:init": "node scripts/init-db.cjs",
    "db:seed": "node scripts/seed-db.cjs",
    "db:seed:demo": "node scripts/seed-demo.cjs",
    "db:migrate": "node scripts/apply-migrations.cjs",
    "check:env": "node scripts/check-env.cjs",
    "test:db": "curl http://localhost:8888/.netlify/functions/db-status || curl http://localhost:4321/.netlify/functions/db-status",
    "test:intents": "curl http://localhost:8888/.netlify/functions/intents-list || curl http://localhost:4321/.netlify/functions/intents-list",
    "test:embeddings": "curl http://localhost:8888/.netlify/functions/embedding-health || curl http://localhost:4321/.netlify/functions/embedding-health",
    "seed": "npm-run-all --parallel seed:*",
    "seed:db": "node ./scripts/seed-db.cjs",
    "seed:demo": "node ./scripts/seed-demo.cjs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^6.3.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@elevenlabs/react": "^0.1.7",
    "@nanostores/react": "^1.0.0",
    "@neondatabase/serverless": "^1.0.1",
    "@netlify/blobs": "^10.0.0",
    "@netlify/edge-functions": "^2.14.5",
    "@netlify/functions": "^4.1.5",
    "@netlify/neon": "^0.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/react-router-dom": "^5.3.3",
    "astro": "^5.9.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cookie": "^0.6.0",
    "drizzle-orm": "^0.44.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "nanostores": "^0.10.3",
    "openai": "^5.3.0",
    "pgvector": "^0.2.1",
    "postgres": "^3.4.4",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.0",
    "react-force-graph-2d": "^1.27.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3"
  },
  "overrides": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.19.0",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.1",
    "npm-run-all": "^4.1.5"
  }
}
